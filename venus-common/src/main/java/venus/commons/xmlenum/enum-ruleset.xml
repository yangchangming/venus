<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE digester-rules PUBLIC "-//Jakarta Apache //DTD digester-rules XML V1.0//EN" "http://jakarta.apache.org/commons/digester/dtds/digester-rules.dtd">
<digester-rules>
    <pattern value="EnumDef">

        <call-method-rule pattern="name" methodname="setName" paramcount="0"/>
        <call-method-rule pattern="resource" methodname="setResource" paramcount="0"/>

        <pattern value="enums/enum">
            <object-create-rule classname="venus.commons.xmlenum.EnumItem"/>
            <set-properties-rule/>
            <set-next-rule methodname="addEnum"/>
        </pattern>
    </pattern>

</digester-rules>