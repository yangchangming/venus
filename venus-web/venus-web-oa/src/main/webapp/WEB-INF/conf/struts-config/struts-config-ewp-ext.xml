<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>

	<action-mappings>

        <action type="udp.ewp.tag.action.TagAction" scope="request" path="/TagAction" parameter="cmd">
            <forward name="toQueryAll" path="/TagAction.do?cmd=queryAll" redirect="true"/>
            <forward name="listPage" path="/jsp/ewp/tag/listTag.jsp"/>
            <forward name="updatePage" path="/jsp/ewp/tag/insertTag.jsp?isModify=1"/>
            <forward name="detailPage" path="/jsp/ewp/tag/detailTag.jsp"/>
        </action>

        <action type="udp.ewp.requests.action.RequestsAction" scope="request" path="/RequestsAction" parameter="cmd">
            <forward name="toQueryAll" path="/RequestsAction.do?cmd=queryAll" redirect="true"/>
            <forward name="listPage" path="/jsp/ewp/requests/listRequests.jsp"/>
            <forward name="updatePage" path="/jsp/ewp/requests/insertRequests.jsp?isModify=1"/>
            <forward name="detailPage" path="/jsp/ewp/requests/detailRequests.jsp"/>
        </action>

        <action type="udp.ewp.link.action.LinkAction" scope="request" path="/LinkAction" parameter="cmd">
            <forward name="toQueryAll" path="/LinkAction.do?cmd=queryAll" redirect="true"/>
            <forward name="listPage" path="/jsp/ewp/link/listLink.jsp"/>
            <forward name="updatePage" path="/jsp/ewp/link/insertLink.jsp?isModify=1"/>
            <forward name="detailPage" path="/jsp/ewp/link/detailLink.jsp"/>
        </action>
        
	</action-mappings>
	
	<!-- 国际化资源文件定义 -->
	<message-resources parameter="ApplicationResources" />

</struts-config>