<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="udp.ewp.requests.model.Requests" table="ewp_requests">
    <id name="id" column="ID" type="java.lang.String" length="19">
      <generator class="assigned"/>
    </id>
		<property name="first_name" column="FIRST_NAME" type="java.lang.String" length="100"/>
		<property name="last_name" column="LAST_NAME" type="java.lang.String" length="100"/>
		<property name="title" column="TITLE" type="java.lang.String" length="100"/>
		<property name="company" column="COMPANY" type="java.lang.String" length="100"/>
		<property name="email" column="EMAIL" type="java.lang.String" length="100"/>
		<property name="phone" column="PHONE" type="java.lang.String" length="100"/>
		<property name="country" column="COUNTRY" type="java.lang.String" length="100"/>
		<property name="website" column="WEBSITE" type="java.lang.String" length="100"/>
		<property name="referer" column="REFERER" type="java.lang.String" length="100"/>
        <!--oracle-->
        <property name="comments" column="COMMENTS" type="org.springframework.orm.hibernate3.support.ClobStringType" length="1048576000"/>
        <!--mysql/sqlserver/db2-->
        <!--
        <property name="comments" column="COMMENTS" type="java.lang.String"/>
        -->
		<property name="create_time" column="CREATE_TIME" type="java.sql.Timestamp" length="100"/>
		</class>
</hibernate-mapping>
