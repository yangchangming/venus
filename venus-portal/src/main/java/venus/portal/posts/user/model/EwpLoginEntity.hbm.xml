<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="udp.ewp.posts.user.model.EwpLoginEntity" table="ewp_login">
        <id name="id" column="ID" type="java.lang.String" length="36">
            <generator class="assigned"/>
        </id>
        <property name="name" column="NAME" type="java.lang.String" length="50" not-null="true" unique="true"/>
        <property name="pwd" column="PWD" type="java.lang.String" length="50" not-null="true"/>
        <property name="enable" column="ENABLE" type="java.lang.String" length="1" not-null="true"/>
        <property name="isadmin" column="ISADMIN" type="java.lang.String" length="1" not-null="true"/>
    </class>
</hibernate-mapping>