<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="udp.ewp.link.model.Link" table="ewp_link">
    <id name="id" column="ID" type="java.lang.String" length="19">
      <generator class="assigned"/>
    </id>
		<property name="category" column="CATEGORY" type="java.lang.String" length="100"/>
		<property name="title" column="TITLE" type="java.lang.String" length="100"/>
        <property name="website" column="WEBSITE" type="java.lang.String" length="100"/>
        <!--oracle-->
        <property name="content" column="CONTENT" type="org.springframework.orm.hibernate3.support.ClobStringType" length="1048576000"/>
        <!--mysql/sqlserver/db2-->
        <!--
        <property name="content" column="CONTENT" type="java.lang.String"/>
        -->
  </class>
</hibernate-mapping>
