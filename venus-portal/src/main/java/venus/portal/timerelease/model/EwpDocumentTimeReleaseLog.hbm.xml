<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="udp.ewp.timerelease.model.EwpDocumentTimeReleaseLog" table="EWP_DOCUMENT_TIME_RELEASE_LOG">
        <id name="id" column="ID" type="java.lang.String" length="19">
            <generator class="assigned"/>
        </id>
        <property name="docId" column="DOC_ID" type="java.lang.String" length="100"/>
        <property name="docReleasedTime" column="DOC_RELEASED_TIME" type="java.sql.Timestamp"/>
        <property name="docReleasedRealTime" column="DOC_RELEASED_REAL_TIME" type="java.sql.Timestamp"/>

    </class>
</hibernate-mapping>
